{
  "coreutils": {
    "type": "cli",
    "programs": ["cp", "chmod", "dd", "df", "du", "ln", "ls", "mkdir", "mv", "rm", "rmdir", "split", "touch"],
    "build_cmd": "cd obj-llvm/src && extract-bc -o {program}.base.bc ./{program}",
    "test_cmd": "./test-harness-generic.sh {program}",
    "libs": "-ldl -lpthread -lselinux -lcap",
    "extra_libs": "libacl libattr",
    "thresholds": {"min_occurrence": 3, "max_values": 5},
    "klee_config": {
      "max_time": 1800,
      "max_memory": 1000,
      "max_solver_time": 30,
      "test_env": "test.env",
      "extra_klee_flags": [],
      "symbolic_inputs": {
        "ls": "--sym-args 0 2 8 --sym-files 1 32",
        "basename": "--sym-args 0 2 8 --sym-files 1 16",
        "touch": "--sym-args 0 2 8 --sym-files 1 16",
        "cp": "--sym-args 2 2 8 --sym-files 2 32",
        "du": "--sym-args 0 1 8 --sym-files 1 32",
        "chmod": "--sym-args 2 2 8 --sym-files 1 16",
        "dd": "--sym-args 0 6 12 --sym-files 2 32 A B",
        "df": "--sym-args 0 2 8",
        "ln": "--sym-args 2 2 8",
        "mkdir": "--sym-args 1 2 8",
        "mv": "--sym-args 2 2 8 --sym-files 2 16",
        "rm": "--sym-args 1 2 8 --sym-files 1 16",
        "rmdir": "--sym-args 1 2 8",
        "split": "--sym-args 0 4 8 --sym-files 1 64",
        "sort": "--sym-args 0 4 8 --sym-stdin 2048 --sym-files 2 4096 A B",
        "wc": "--sym-args 0 6 10 --sym-stdin 4096 --sym-files 3 4096 A B C",
        "tail": "--sym-args 0 6 12 --sym-stdin 4096 --sym-files 3 4096 -- A B C",
        "head": "--sym-args 0 4 8 --sym-files 1 32",
        "cat": "--sym-args 0 2 8 --sym-files 1 32",
        "chcon": "--sym-args 1 2 8 --sym-files 1 16",
        "chroot": "--sym-args 1 2 8 --sym-files 1 16",
        "shred": "--sym-files 1 32 --sym-args 0 1 8",
        "csplit": "--sym-args 1 2 8 --sym-files 1 32",
        "cut": "--sym-args 0 4 8 --sym-files 1 32",
        "date": "--sym-args 0 2 8 --sym-files 1 16",
        "env": "--sym-args 0 2 8 --sym-files 1 16",
        "expr": "--sym-args 0 2 8 --sym-files 1 16",
        "factor": "--sym-args 0 2 8 --sym-files 1 16",
        "fmt": "--sym-args 0 2 8 --sym-files 1 16",
        "fold": "--sym-args 0 2 8 --sym-files 1 16",
        "groups": "--sym-args 0 2 8 --sym-files 1 16",
        "help": "--sym-args 0 2 8 --sym-files 1 16",
        "id": "--sym-args 0 2 8 --sym-files 1 16",
        "join": "--sym-args 2 2 8 --sym-files 1 32",
        "misc": "--sym-args 0 2 8 --sym-files 1 16",
        "nice": "--sym-args 1 2 8 --sym-files 1 16",
        "numfmt": "--sym-args 0 2 8 --sym-files 1 16",
        "od": "--sym-args 0 2 8 --sym-files 1 16",
        "paste": "--sym-args 0 2 8 --sym-files 1 16",
        "pathchk": "--sym-args 0 2 8 --sym-files 1 16",
        "pr": "--sym-args 0 2 8 --sym-files 1 16",
        "printenv": "--sym-args 0 2 8 --sym-files 1 16",
        "printf": "--sym-args 0 2 8 --sym-files 1 16",
        "ptx": "--sym-args 0 2 8 --sym-files 1 16",
        "pwd": "--sym-args 0 2 8 --sym-files 1 16",
        "readlink": "--sym-args 0 2 8 --sym-files 1 16",
        "realpath": "--sym-args 0 2 8 --sym-files 1 16",
        "runcon": "--sym-args 1 2 8 --sym-files 1 16",
        "seq": "--sym-args 0 2 8 --sym-files 1 16",
        "sha1sum": "--sym-args 0 2 8 --sym-files 1 16",
        "sha224sum": "--sym-args 0 2 8 --sym-files 1 16",
        "sha256sum": "--sym-args 0 2 8 --sym-files 1 16",
        "sha384sum": "--sym-args 0 2 8 --sym-files 1 16",
        "sha512sum": "--sym-args 0 2 8 --sym-files 1 16",
        "shuf": "--sym-args 0 2 8 --sym-files 1 16",
        "sleep": "--sym-args 0 2 8 --sym-files 1 16",
        "stat": "--sym-args 1 1 8",
        "sum": "--sym-args 0 2 8 --sym-files 1 16",
        "sync": "--sym-args 0 2 8 --sym-files 1 16",
        "tac": "--sym-args 0 2 8 --sym-files 1 16",
        "tee": "--sym-args 0 2 8 --sym-files 1 16",
        "test": "--sym-args 0 2 8 --sym-files 1 16",
        "tr": "--sym-args 0 2 8 --sym-files 1 16",
        "true": "--sym-args 0 2 8 --sym-files 1 16",
        "truncate": "--sym-args 0 2 8 --sym-files 1 16",
        "tsort": "--sym-args 0 2 8 --sym-files 1 16",
        "tty": "--sym-args 0 2 8 --sym-files 1 16",
        "uname": "--sym-args 0 2 8 --sym-files 1 16",
        "unexpand": "--sym-args 0 2 8 --sym-files 1 16",
        "uniq": "--sym-args 0 2 8 --sym-files 1 16",
        "unlink": "--sym-args 0 2 8 --sym-files 1 16",
        "vdir": "--sym-args 0 2 8 --sym-files 1 16",
        "whoami": "--sym-args 0 2 8 --sym-files 1 16",
        "yes": "--sym-args 0 2 8 --sym-files 1 16"
      }
    }
  },
  "apr": {
    "type": "library",
    "programs": ["apr_test"],
    "build_cmd": "./configure CC=clang CFLAGS='-g -O0' && make",
    "driver": "drivers/apr_driver.c",
    "libs": "-lapr-1",
    "thresholds": {"min_occurrence": 3, "max_values": 10},
    "klee_config": {
      "max_time": 1800,
      "max_memory": 1000,
      "max_solver_time": 30,
      "test_env": null,
      "extra_klee_flags": [],
      "symbolic_inputs": {
        "apr_test": "--sym-args 0 2 8 --sym-files 1 32"
      }
    }
  },
  "m4": {
    "type": "cli",
    "programs": ["m4"],
    "build_cmd": "./configure CC=clang CFLAGS='-g -O0' && make",
    "test_cmd": "make check",
    "libs": "-ldl",
    "thresholds": {"min_occurrence": 5, "max_values": 8},
    "klee_config": {
      "max_time": 1800,
      "max_memory": 1000,
      "max_solver_time": 30,
      "test_env": null,
      "extra_klee_flags": [],
      "symbolic_inputs": {
        "m4": "--sym-args 0 2 8 --sym-files 1 32"
      }
    }
  },
  "busybox": {
    "type": "multi",
    "programs": ["echo", "cat", "ls", "mkdir", "rm"],
    "build_cmd": "make defconfig && make CC=clang",
    "test_cmd": "./testsuite/runtest {program}",
    "libs": "-lm",
    "thresholds": {"min_occurrence": 3, "max_values": 5},
    "klee_config": {
      "max_time": 1800,
      "max_memory": 1000,
      "max_solver_time": 30,
      "test_env": null,
      "extra_klee_flags": [],
      "symbolic_inputs": {
        "echo": "--sym-args 0 2 8",
        "cat": "--sym-args 0 2 8 --sym-files 1 32",
        "ls": "--sym-args 0 2 8 --sym-files 1 32",
        "mkdir": "--sym-args 1 2 8",
        "rm": "--sym-args 1 2 8 --sym-files 1 16"
      }
    }
  }
}
